{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@apps/*": ["src/apps/*"],
      "@backend/*": ["src/backend/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@themes/*": ["src/themes/*"]
    }
  }
}